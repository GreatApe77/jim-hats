{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "npx tsx --watch src/index.ts",
    "test": "npx vitest --watch=false --reporter=verbose --exclude=**/*.e2e.test.ts",
    "test:integration": "chmod +x ./scripts/run-integration-tests.sh && ./scripts/run-integration-tests.sh",
    "coverage": "npx vitest run --coverage --watch=false --reporter=verbose",
    "coverage:integration": "chmod +x ./scripts/run-integration-coverage.sh && ./scripts/run-integration-coverage.sh",
    "migrate": "npx prisma migrate deploy",
    "start:db": "cd .. && docker compose --file docker-compose-dev.yml up -d  && sleep 2 && cd ./server && npm run migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.2",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.6.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.5.0",
    "globals": "^15.6.0",
    "prisma": "^5.15.0",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.6",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.1",
    "vitest": "^1.6.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "dependencies": {
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "zod": "^3.23.8"
  }
}
